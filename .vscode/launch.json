{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "program": "${workspaceFolder}/_projects/rates/model/hello.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "name": "debugger",
      "type": "node",
      "request": "launch",
      "args": ["wdio.conf.js", "baseUrl=https://www.saucedemo.com", "tags=@demo", "spec=test/features/saucedemo/saucedemo.feature"],
      "cwd": "${workspaceFolder}",
      "autoAttachChildProcesses": true,
      "program": "${workspaceRoot}/node_modules/@wdio/cli/bin/wdio.js",
      "console": "integratedTerminal"
    }
  ]
}
